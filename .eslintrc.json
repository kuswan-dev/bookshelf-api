{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "comma-dangle": "off",
        "indent": ["error", 4],
        "import/extensions": "off",
        "import/prefer-default-export": "off",
        "no-else-return": "off",
        "semi": "off"
    }
}
